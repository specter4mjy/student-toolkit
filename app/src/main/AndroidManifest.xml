<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ml.mk.jm.ay.ak.studenttoolkit" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ToDoActivity"
            android:label="@string/title_activity_to_do"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".EditToDoActivity"
            android:label="@string/title_activity_edit_to_do"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity android:name=".ShowToDoActivity" >
        </activity>
        <activity android:name=".NewToDoActivity" >
        </activity>
        <activity
            android:name=".PostponeToDo_Activity"
            android:label="@string/title_activity_postpone_to_do_"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
